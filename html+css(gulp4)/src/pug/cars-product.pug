						+item("cars-category-2.html", "73", "car", "Demetrisfort", "From Rs. 1,461 per day")
						+item("cars-category-2.html", "11", "car", "South Virgilland", "From Rs. 1,461 per day")
						+item("cars-category-2.html", "56", "car", "Purdyhaven", "From Rs. 1,461 per day")
