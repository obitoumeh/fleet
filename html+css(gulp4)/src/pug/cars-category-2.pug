---
title: Fleet
classCars: active
---
extends layouts/layout
block content
	.section-mb80.main.main_cars-category
		.main__bg
			img(src="img/content/bg-flight-1.jpg" alt="Main")
		.main__center.center
			.panel.panel_cars-category
				.panel__background
				.panel__controls
					button.button-stroke.button-small.panel__button.active Return to same location
					button.button-stroke.button-small.panel__button Return to same different location
				.panel__body
					.panel__row
						+location("location_small location_down", "marker", "Pickup location")
						+daterange("datepicker_small datepicker_down", "ddd, MMM DD", "calendar", "Check in", "Check out")
						button.panel__search
							+i('search')
							span Search
	.section.catalog
		.sorting
			.sorting__center.center
				.control
					a.button-stroke.button-small.control__button(href="cars.html")
						+i('arrow-left')
						span Go home
					ul.breadcrumbs
						li.breadcrumbs__item
							a.breadcrumbs__link(href="index.html") Home
						li.breadcrumbs__item
							a.breadcrumbs__link(href="index.html") Stays
						li.breadcrumbs__item
							a.breadcrumbs__link(href="stays-category.html") New Zealand
						li.breadcrumbs__item South Island
				.sorting__body
					.sorting__box
						h2.sorting__title.h2 24 locations found
						.sorting__line
							.sorting__sale up to 25% off
							.sorting__details May 1 - 14, 2 guests
					.sorting__location
						button.button-stroke.sorting__button.js-sorting-button
							span Show map
							+i('arrow-bottom')
						.sorting__map.js-sorting-map
							iframe(src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d63817.0803287881!2d168.63234961382247!3d-45.04173987887954!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0xa9d51df1d7a8de5f%3A0x500ef868479a600!2z0JrRg9C40L3RgdGC0LDRg9C9LCDQndC-0LLQsNGPINCX0LXQu9Cw0L3QtNC40Y8!5e0!3m2!1sru!2sua!4v1624887132616!5m2!1sru!2sua" width="600" height="450" loading="lazy")
				.sorting__foot
					.nav
						a.nav__link.active(href="#") Sightseeing
						a.nav__link(href="#") Transportation activities
						a.nav__link(href="#") Art and culture
					.sorting__select.tablet-show
						select.select
							option Sightseeing
							option Transportation activities
							option Art and culture
					.sorting__select
						select.select
							option Time of day
							option Time of week
		.catalog__body
			.catalog__center.center
				.catalog__list
					+card("cars-product.html", "img/content/cars-photo-1@2x.jpg", "img/content/cars-photo-1.jpg", "category card__category", "excellent", "Vauxhall Corsa", "/DAY", "$267", "route", "London - Kings Cross", false, false, "$200 total", "4.8", "12", false, fasle, "card_car")
